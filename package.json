{
	"name": "gui-git-editor",
	"private": true,
	"version": "25.12.104",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"tauri": "tauri",
		"tauri:dev": "tauri dev",
		"tauri:build": "npm run format && npm run tauri:fmt && tauri build",
		"tauri:build:debug": "tauri build --debug",
		"tauri:fmt": "cd src-tauri && cargo fmt",
		"tauri:update": "npm update @tauri-apps/cli @tauri-apps/api @tauri-apps/plugin-cli @tauri-apps/plugin-opener",
		"tauri:update:cargo": "cd src-tauri && cargo update",
		"tauri:update:all": "npm run tauri:update && npm run tauri:update:cargo",
		"lint": "biome lint --write .",
		"format": "biome format --write .",
		"check": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:rust": "cd src-tauri && cargo test",
		"test:all": "npm run test && npm run test:rust"
	},
	"dependencies": {
		"@dnd-kit/core": "^6.3.1",
		"@dnd-kit/modifiers": "^9.0.0",
		"@dnd-kit/sortable": "^10.0.0",
		"@dnd-kit/utilities": "^3.2.2",
		"@headlessui/react": "^2.2.9",
		"@heroicons/react": "^2.2.0",
		"@tauri-apps/api": "^2",
		"@tauri-apps/plugin-cli": "^2.4.1",
		"@tauri-apps/plugin-log": "^2.7.1",
		"@tauri-apps/plugin-opener": "^2",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"zustand": "^5.0.9"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@tailwindcss/vite": "^4.1.18",
		"@tauri-apps/cli": "^2",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/ui": "^4.0.16",
		"jsdom": "^27.4.0",
		"tailwindcss": "^4.1.18",
		"typescript": "~5.9.3",
		"vite": "^7.3.0",
		"vitest": "^4.0.16"
	}
}
